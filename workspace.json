{
  "version": 2,
  "projects": {
    "clients": "libs/clients",
    "components": "libs/components",
    "contexts": {
      "root": "libs/contexts",
      "sourceRoot": "libs/contexts/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/contexts/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/contexts"],
          "options": {
            "jestConfig": "libs/contexts/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "dome-solar-calepinage": {
      "root": "apps/dome-solar-calepinage",
      "sourceRoot": "apps/dome-solar-calepinage",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": "production",
          "options": {
            "root": "apps/dome-solar-calepinage",
            "outputPath": "dist/apps/dome-solar-calepinage"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "dome-solar-calepinage:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "dome-solar-calepinage:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "dome-solar-calepinage:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dome-solar-calepinage"],
          "options": {
            "jestConfig": "apps/dome-solar-calepinage/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "apps/dome-solar-calepinage/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      },
      "tags": []
    },
    "dome-solar-calepinage-e2e": {
      "root": "apps/dome-solar-calepinage-e2e",
      "sourceRoot": "apps/dome-solar-calepinage-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/dome-solar-calepinage-e2e/cypress.json",
            "devServerTarget": "dome-solar-calepinage:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dome-solar-calepinage:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/dome-solar-calepinage-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["dome-solar-calepinage"]
    },
    "dots-app": {
      "root": "apps/dots-app",
      "sourceRoot": "apps/dots-app",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": "production",
          "options": {
            "root": "apps/dots-app",
            "outputPath": "dist/apps/dots-app"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "dots-app:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "dots-app:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "dots-app:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/dots-app"],
          "options": {
            "jestConfig": "apps/dots-app/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/dots-app/**/*.{ts,tsx,js,jsx}"]
          }
        }
      },
      "tags": []
    },
    "dots-app-e2e": {
      "root": "apps/dots-app-e2e",
      "sourceRoot": "apps/dots-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/dots-app-e2e/cypress.json",
            "devServerTarget": "dots-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "dots-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/dots-app-e2e/**/*.{js,ts}"]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["dots-app"]
    },
    "dots-system": {
      "root": "libs/dots-system",
      "sourceRoot": "libs/dots-system/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/dots-system/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/dots-system"],
          "options": {
            "jestConfig": "libs/dots-system/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "form-builder": "libs/form-builder",
    "hooks": "libs/hooks",
    "lcdb-back": "apps/lcdb-back",
    "lcdb-front": "apps/lcdb-front",
    "schema": {
      "root": "libs/schema",
      "sourceRoot": "libs/schema/src",
      "projectType": "library",
      "tags": [],
      "targets": {
        "build": {
          "executor": "@nrwl/web:rollup",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/schema",
            "tsConfig": "libs/schema/tsconfig.lib.json",
            "project": "libs/schema/package.json",
            "entryFile": "libs/schema/src/index.ts",
            "external": ["react/jsx-runtime"],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "compiler": "babel",
            "assets": [
              {
                "glob": "libs/schema/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/schema/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/schema"],
          "options": {
            "jestConfig": "libs/schema/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "schema--to-delete": "libs/schema--to-delete",
    "theme": "libs/theme",
    "tokens": {
      "root": "libs/tokens",
      "sourceRoot": "libs/tokens/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/js:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/tokens",
            "main": "libs/tokens/src/index.ts",
            "tsConfig": "libs/tokens/tsconfig.lib.json",
            "assets": ["libs/tokens/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/tokens/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/tokens"],
          "options": {
            "jestConfig": "libs/tokens/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": ["tokens"]
    },
    "utils": "libs/utils"
  }
}
